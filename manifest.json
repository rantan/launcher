{
    "manifest_version": 2,
    "name": "launcher",
    "version": "0.1.0",
    "description": "simple launcher",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [

    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "hello.js"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ]
}